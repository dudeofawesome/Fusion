name: 'Arduino'
extensions: [
    '.ino'
]
commandSequence: [
    {
        command: 'arduino'
        arguments: ['-build', '{{file}}']
    }
]
variants: [
    {
        name: 'Upload'
        commandSequence: [
            {
                command: 'arduino'
                arguments: ['-upload', '{{file_base_name}}']
            }
        ]
    }
]
